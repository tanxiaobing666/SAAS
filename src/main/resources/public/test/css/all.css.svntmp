@import "index.css";
@import "tabs.css";
@import "panel.css";
@import "portal.css";
@import "tooltip.css"; 

.nopadding{
	padding:0px;
	margin:0px;
}

.noborder{
	border-top: 0px;
	border-left: 0px;
	border-right: 0px;
	border-bottom: 0px;
}
.checked{
	border: 1px solid #000;
	background:#000 url(../images/invalid_line.gif) repeat-x bottom;
}

span.money{
	color:green;
	padding-left:2px;
	padding-right:2px;
}

/************UL��ʽ*********************/
ul.group {
	margin-left: 0;
	padding-left: 0;
	display: inline;
	list-style: none;
	
}

.group-menus:before, 
ul.group:before, 
.group-menus:after,
ul.group:after{
	content: '';
	display: table;
	clear: both;
}

ul.group li {
	margin: 2px 4px;
	cursor: pointer;
	padding: 5px;
	border: 1px solid #999;
	float: left;
	display: inline;
}

ul.group li:hover {
	padding: 4px;
	border: 2px solid #66f;
}

ul.group li.selected {
	padding: 4px;
	border: 2px solid #66f;
	background: url("selected.png") no-repeat right bottom
		rgb(255, 255, 255);
	cursor: default;
}
/************Table�б���ʽ*********************/
.navbar {
	margin-left: 0;
	padding-left: 0;
	text-align: left;
	width: 100%;
	height:30px;
}

.navbar ul {
	margin-left: 0;
	padding-left: 0;
	display: inline;
	list-style: none;
}

.navbar ul li {
	margin-left: 0;
	margin-bottom: 0;
	padding: 2px;
	list-style: none;
	display: inline;
	color: #666;
}

.navbar ul li:hover {
	padding: 2px;
	color: #000;
}

/************Table�б���ʽ*********************/
.ui-table {
	width: 100%;
	font-size: 13px;
}

.ui-table>thead th {
	background-color: #eee;
/*	text-align: left; */
	color: #555;
	margin: 0px;
	padding: 5px 8px; 
}

.ui-table>thead th h3 {
	padding: 0px;
	margin: 0px; 
	font-size: 13px;
}

.ui-table td {
	padding: 4px 6px 6px;
	/* border-bottom: 1px solid #c6d5e1; */
}

.ui-table .head {
	background: #eee url(../../images/sort/sort.gif ) 7px center no-repeat;
	cursor: pointer;
	padding-left: 18px;
}

.ui-table .desc,.ui-table .asc {
	background-color: #bbb;
}

.ui-table>tbody tr:hover td {
	/* border-bottom: 1px solid #6c95ff; */
}

.ui-table .desc { 
	background: #bbb url(../../images/sort/desc.gif) 7px center no-repeat;
	cursor: pointer;
	padding-left: 18px
}

.ui-table .asc { 
	background: #bbb url("../../images/sort/asc.gif") 7px center no-repeat;
	cursor: pointer;
	padding-left: 18px;
}

.ui-table th a {
	color: #fff;
	text-decoration: none;
}

.ui-table .head:hover,.ui-table .desc:hover,.ui-table .asc:hover {
	color: #000;
}

.ui-table>tbody tr {
	background: #fff;
}

.ui-table>tbody tr:nth-child(even),.ui-table tbody tr td.evenrow {
	background: #f0f0f0;
}

.ui-table>tbody tr:hover {
	background: #ffa;
}

.ui-table td.evenselected {
	background: #ecf2f6
}

.ui-table td.oddselected {
	background: #dce6ee
}

/************Table����ʽ*********************/
.x-form-invalid,textarea.x-form-invalid {
	background: url("../../images/invalid_line.gif") repeat-x scroll center
		bottom rgb(255, 255, 255);
	border: 2px solid rgb(221, 120, 112);
}

.x-form-text {
	vertical-align: middle;
}

.x-form-warn {
	float: right;
	display: inline-block;
	background-repeat: no-repeat;
	background-image: url("../../images/exclamation.gif");
	background-position: left bottom;
	width: auto;
	padding-left: 20px;
	padding-top: 22px;
}

.pao-shadow {
	padding: 2px;
	margin: 0px;
	min-height: 35px;
	-moz-border-radius: 5px 5px 5px 5px;
	-webkit-border-radius: 5px 5px 5px 5px;
	border-radius: 5px 5px 5px 5px;
	background-color: rgba(200, 200, 200, 0.8);
}

.pao_msg {
	display: block;
	min-width: 130px;
	min-height: 30px;
	color: #000;
	padding: 2px;
	background-color: rgba(255, 255, 255, 0.7);
}

.pao_tag { /* background-color: #fff; */
	background-repeat: no-repeat;
	background-image: url("../../images/pao_bg.png");
}

.pao_tag.leftBottom {
	position: absolute;
	background-position: -10px;
	left: -15px;
	top: 5px;
	padding-left: 20px;
	padding-top: 30px;
}

#window_layer {
	overflow: hidden;
	left: 0;
	top: 0;
	z-index: 1103;
	width: 100%;
	height: 100%;
	background: rgba(0, 0, 0, 0.1);
	position: absolute;
}


.accountPanel-header,.panel-header {
	font-weight: bold;
	padding: 5px 5px 5px 10px;
	color: #fff;
	filter: progid : DXImageTransform.Microsoft.Gradient (startColorStr = rgb(14, 44, 80), endColorStr =rgb(187, 219, 241), gradientType ='1' );
	background: -webkit-gradient(linear, 0% 0%, 100% 0%, from(rgb(14, 44, 80)), to(rgb(187, 219, 241) ) );
	background: -moz-linear-gradient(left, rgb(14, 44, 80),rgb(187, 219, 241) );
	background: -o-linear-gradient(left, rgb(14, 44, 80), rgb(187, 219, 241));
	text-align: left;
	background-color: #ddd;
}

.accountPanel-content {
	padding-bottom: 20px;
}

/******************box shadow********************************/

.ui-shadow {
	-moz-box-shadow: 1px 3px 10px /*{global-box-shadow-size}*/ 			rgba(0,0,0,.3) /*{global-box-shadow-color}*/;
	-webkit-box-shadow: 1px 3px 10px /*{global-box-shadow-size}*/ 		rgba(0,0,0,.3) /*{global-box-shadow-color}*/;
	box-shadow: 1px 3px 10px /*{global-box-shadow-size}*/ 				rgba(0,0,0,.3) /*{global-box-shadow-color}*/;
}

