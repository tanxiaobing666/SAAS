}
.ui-corner-bl {
	-moz-border-radius-bottomleft: 		.4em ;
	-webkit-border-bottom-left-radius: 	.4em ;
	border-bottom-left-radius: 			.4em ;
}
.ui-corner-br {
	-moz-border-radius-bottomright: 	.4em ;
	-webkit-border-bottom-right-radius: .4em ;
	border-bottom-right-radius: 		.4em ;
}
.ui-corner-top {
	-moz-border-radius-topleft: 		.4em ;
	-webkit-border-top-left-radius: 	.4em ;
	border-top-left-radius: 			.4em ;
	-moz-border-radius-topright: 		.4em ;
	-webkit-border-top-right-radius: 	.4em ;
	border-top-right-radius: 			.4em ;
}
.ui-corner-bottom {
	-moz-border-radius-bottomleft: 		.4em ;
	-webkit-border-bottom-left-radius: 	.4em ;
	border-bottom-left-radius: 			.4em ;
	-moz-border-radius-bottomright: 	.4em ;
	-webkit-border-bottom-right-radius: .4em ;
	border-bottom-right-radius: 		.4em ;
	}
.ui-corner-right {
	-moz-border-radius-topright: 		.4em ;
	-webkit-border-top-right-radius: 	.4em ;
	border-top-right-radius: 			.4em ;
	-moz-border-radius-bottomright: 	.4em ;
	-webkit-border-bottom-right-radius: .4em ;
	border-bottom-right-radius: 		.4em ;
}
.ui-corner-left {
	-moz-border-radius-topleft: 		.4em ;
	-webkit-border-top-left-radius: 	.4em ;
	border-top-left-radius: 			.4em ;
	-moz-border-radius-bottomleft: 		.4em ;
	-webkit-border-bottom-left-radius: 	.4em ;
	border-bottom-left-radius: 			.4em ;
}
.ui-corner-all {
	-moz-border-radius: 				.4em ;
	-webkit-border-radius: 				.4em ;
	border-radius: 						.4em ;
}
.ui-corner-none {
	-moz-border-radius: 				   0;
	-webkit-border-radius: 				   0;
	border-radius: 						   0;
}

button.fix05{
	padding:5px 5px;
}

button.fix15{
	padding:5px 15px;
}

button{
	border: 1px solid 		#888 ;
	font-weight: bold;
	color: 					#000 ;
	font-size: 14px;
	line-height:20px; 
	padding: 5px 15px;
	background: #EEE;
	margin: 1px 0;
	outline: none;
	cursor: pointer;
}
button:active {
	border: 1px solid 		#888 /*{a-bup-border}*/;
	background: 			#ccc /*{a-bup-background-color}*/;
	font-weight: bold;
	color: 					#000 /*{a-bup-color}*/; 
	text-shadow: 0 1px  1px  #eee ;
	background-image: -webkit-gradient(linear, left top, left bottom, from( #fff ), to( #ff8 )); /* Saf4+, Chrome */
	background-image: -webkit-linear-gradient( #fff , #ff8 ); /* Chrome 10+, Saf5.1+ */
	background-image:    -moz-linear-gradient( #fff , #ff8 ); /* FF3.6 */
	background-image:     -ms-linear-gradient( #fff , #ff8 ); /* IE10 */
	background-image:      -o-linear-gradient( #fff , #ff8 ); /* Opera 11.10+ */
	background-image:         linear-gradient( #fff , #ff8 );
	
	-webkit-box-shadow: #555 0 .1em 1px, /* border */ rgba(0,0,0,0.3) 0 .2em 6px; /* drop shadow */
	-moz-box-shadow: 	#555 0 .1em 1px, /* border */ rgba(0,0,0,0.3) 0 .2em 6px; /* drop shadow */
	box-shadow: 		#555 0 .1em 1px, /* border */ rgba(0,0,0,0.3) 0 .2em 6px; /* drop shadow */
}

