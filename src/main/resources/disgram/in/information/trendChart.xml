<trans name="in/information/trendChart" desc="资讯趋势图（次数/人数）" busi="6">
	<snd>
		<field name="BANK_ID" desc="企业ID" type="C" comment="" required="false"/>
		<field name="CORP_ID" desc="企业微信编号" type="C" comment="" required="true"/>
		<field name="START_TIME" desc="起始时间" type="C" comment="" required="false"/>
		<field name="END_TIME" desc="结束时间" type="C" comment="" required="false"/>

	</snd>
	<rcv>
		<field name="BROWSE_SUM" desc="浏览总数" type="C" comment="" />
		<field name="VISITOR_COUNT" desc="浏览总人数" type="C" comment="" />
		<field name="SUM_PERCENT" desc="浏览总数同比上周" type="C" comment="" />
		<field name="VISITOR_PERCENT" desc="浏览人数同比上周" type="C" comment="" />
		<field-list name="TREND_LIST" desc="资讯趋势图列表" type="E">
			<field name="BROWSE_TIME" desc="浏览时间" type="C" comment="" />
			<field name="WEEK_NAME" desc="浏览星期" type="C" comment="" />
			<field name="BROWSE_NUM" desc="浏览次数" type="C" comment="" />
			<field name="BROWSE_VISITOR" desc="浏览人数" type="C" comment="" />
		</field-list>
	</rcv>
</trans>