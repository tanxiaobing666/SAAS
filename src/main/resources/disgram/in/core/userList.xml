<?xml version="1.0" encoding="utf-8"?>
<trans name="core/userList" desc="用户-查询" busi="6">
	<snd>
		<field name="ORG_ID" desc="机构编号"  />
		<field name="USER_NO" desc="用户编号"   />
		<field name="USER_NAME" desc="登陆名" />
		<field name="ROLE_ID" desc="用户角色"  />
		<field name="NEXT_KEY" desc="起始位置数值" />
		<field name="PAGE_SIZE" desc="每页数量"  />
	</snd>
	<rcv>
	    <field name="TOTAL_NUM" desc="总页数"  />
		<field name="HAS_NEXT" desc="结束标志"  comment="Y最后一页，N可继续查询" />
		<field-list name="LIST" desc="用户列表" type="E"  >
			<field name="USER_ID" desc="用户登录账号" />
			<field name="USER_NAME" desc="用户姓名"  />
			<field name="IDT_NO"  desc="证件号码" />
			<field name="MOBILE" desc="手机号码"  />
			<field name="SEX" desc="性别"  />
			<field name="STAT" desc="用户状态"   comment="1-正常；2-停用；" />
			<field name="ORG_ID" desc="机构ID" />
			<field name="EMAIL" desc="邮箱"   />
			<field name="ORG_ID_TXT" desc="机构名称"/>
			<field name="PARENT_ID"  desc="父编号"/>
			<field name="ORG_CODE" desc="机构编号" />
			<field name="IDT_TYPE"   desc="证件类型"/>
			<field name="ROLE_NAME" desc="用户角色"   />	
		</field-list>
	</rcv>
</trans>