<?xml version="1.0" encoding="utf-8"?>
<trans name="newsChnlList" desc="渠道资讯列表展示" busi="6">
    <!-- 请求头 -->
    <snd>
        <field name="CLS_ID" desc="内部分类ID" typ="C"/>
    </snd>

    <!-- 响应头 -->
    <rcv>
        <field name="NEWS_CLS_NAME" desc="平台资讯名称" typ="C"/>
        <field name="NEWS_CLS_ID" desc="平台分类ID" typ="C"/>
        <filed-list xpath="LIST" name="LIST" type="E">
            <field name="CHNL_NO" desc="渠道ID" typ="C"/>
            <field name="CHNL_NAME" desc="渠道名称" typ="C"/>
            <filed-list name="CHNLLIST" type="E">
                <field name="CLS_ID" desc="渠道分类ID" typ="C"/>
                <field name="CLS_NAME" desc="渠道分类名称" typ="C"/>
            </filed-list>
        </filed-list>
    </rcv>
</trans>
